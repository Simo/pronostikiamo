<%= form_for @game do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :round_id %><br />
    <%= f.text_field :round_id %>
  </p>
  <p>
    <%= f.label :home_team_id %><br />
    <%= f.text_field :home_team_id %>
  </p>
  <p>
    <%= f.label :away_team_id %><br />
    <%= f.text_field :away_team_id %>
  </p>
  <p>
    <%= f.label :codfip %><br />
    <%= f.text_field :codfip %>
  </p>
  <p>
    <%= f.label :result %><br />
    <%= f.text_field :result %>
  </p>
  <p><%= f.submit %></p>
<% end %>
