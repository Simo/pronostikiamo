<% if @giocate.blank? %>
	$('li').removeClass("evidenziato");
	$('li#campionato_<%= @campionato %>').addClass("evidenziato");
	$('#schedina').slideUp('fast').slideDown('slow').html("<%= escape_javascript(render(:partial => "giornata"))%>");
<% else %>
	$('li').removeClass("evidenziato");
	$('li#campionato_<%= @campionato %>').addClass("evidenziato");
	$('#schedina').slideUp('fast').slideDown('slow').html("<%= escape_javascript(render(:partial => "valutato"))%>");
<% end %>
